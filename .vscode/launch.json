	"version": "0.2.0",
	"configurations": [
		{
			"type": "lldb",
			"request": "launch",
			"name": "bench_server",
			"cargo": {
				"args": [
					"build",
					"--bin=bench_server",
				],
				"filter": {
					"name": "bench_server",
					"kind": "bin"
				}
			},
			"env": {
				"RUST_LOG": "info"
			},
			"args": [
				"--partitions",
				"1"
			],
			"cwd": "${workspaceFolder}",
		},
		{
			"type": "lldb",
			"request": "launch",
			"name": "bench_client",
			"cargo": {
				"args": [
					"build",
					"--bin=bench_client",
				],
				"filter": {
					"name": "bench_client",
					"kind": "bin"
				}
			},
			"env": {
				"RUST_LOG": "info"
			},
			"args": [],
			"cwd": "${workspaceFolder}"
		}
	]
}